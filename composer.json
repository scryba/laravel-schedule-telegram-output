    }
  ],
  "homepage": "https://michael.laweitech.com/projects/laravel-schedule-telegram-output",
  "readme": "README.md",
  "support": {
    "issues": "https://github.com/scryba/laravel-schedule-telegram-output/issues",
    "source": "https://github.com/scryba/laravel-schedule-telegram-output"
  },
  "funding": [
    {
      "type": "custom",
